<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title> 
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" >
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" ></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" ></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" ></script>
</head>
<body>
    <form action="/order" method="get">
    <div class="container">
		<nav class="navbar navbar-expand-lg  navbar-dark bg-dark">
            <a class="navbar-brand" href="#">商品管理系统</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse"
                data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
        
            <div>			
				<input type="submit" name="a1" class="btn btn-outline-success my-2 my-sm-0" value="注销">
			</div>
        </nav>

		<div class="row" style="margin-top: 10px">
        <!-- 左边栏 -->
        <div class="col-md-2">
            <div class="list-group" id="tabs_nav">
                <input type="submit" name="a1" class="list-group-item list-group-item-action" value="首页">
                <input type="submit" name="a1" class="list-group-item list-group-item-action" value="商品列表">
                <input type="submit" name="a1" class="list-group-item list-group-item-action" value="增加商品">
                <input type="submit" name="a1" class="list-group-item list-group-item-action" value="进货记录">
                <input type="submit" name="a1" class="list-group-item list-group-item-action" value="用户列表"> 
                
            </div>
        </div>			
		<!-- 主内容区 -->
		<div class="col-md">
			<div class="card">
					<div class="card-header">进货单
					</div>
					<div class="card-body">
						<form>
							<div class="form-group row">
								<label for="inputEmail3" class="col-sm-2 col-form-label">商品名称:</label>
								<div class="col-sm">
									<input type="text" name="pname" class="form-control" 
										value="<%=order[0].pname%>">
								</div>
							</div>
							<div class="form-group row">
								<label for="inputPassword3" class="col-sm-2 col-form-label">进货量:</label>
								<div class="col-sm">
									<input type="text" name="onumber" class="form-control" 
									placeholder="请输入进货量">
								</div>
							</div>
						
						<div class="form-group row">
								<label for="inputPassword3" class="col-sm-2 col-form-label">供应商:</label>
								<div class="col-sm">
									<input type="text" name="pfirm" class="form-control" 
									placeholder="请输入供应商">
								</div>
							</div>																															
							<div class="form-group row">
								<div class="col-sm-10">
                                    <input name="a1" type="submit" style="margin-left:390px" value="确认">
                                    <input name="a1" type="submit" style="margin-left:10px" value="取消">
								</div>
							</div>
						</form>
					</div>
				</div>

			</div>					
		</div>
		</div>
    </form>     
</body>
</html>